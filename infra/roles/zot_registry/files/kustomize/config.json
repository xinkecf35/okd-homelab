{
  "storage": {
    "rootDirectory": "/var/lib/registry"
  },
  "http": {
    "address": "0.0.0.0",
    "port": "443",
    "tls": {
      "cert": "/etc/ssl/certs/zot/zot-registry.crt",
      "key": "/etc/ssl/private/zot/zot-registry.key"
    }
  },
  "log": {
    "level": "debug"
  },
  "extensions": {
    "search": {
      "enable": true
    },
    "ui": {
      "enable": true
    }
  }
}
